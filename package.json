{
  "name": "octon",
  "private": true,
  "scripts": {
    "dev": "concurrently \"tsc -p tsconfig.server.json --watch\" \"tsc -p tsconfig.client.json --watch\" \"nodemon -r dotenv/config build/server/index.js --watch build/server\"",
    "build": "rm -rf build && npm run build-server && npm run build-client && npm run build-functions",
    "build-client": "tsc -p tsconfig.client.json && next build ./build/client",
    "build-server": "tsc -p tsconfig.server.json",
    "build-functions": "tsc -p ./functions/tsconfig.json && node ./functions/replace-env",
    "start": "NODE_ENV=production node build/server/index.js",
    "format": "prettier --single-quote --trailing-comma es5 --write \"{server,client,functions}/**/*.{ts,tsx}\"",
    "lint": "tslint functions/**/*.ts && tslint server/**/*.ts client/**/*.tsx",
    "test": "npm run lint && npm run build"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "clipboard-js": "^0.3.5",
    "cookie-parser": "^1.4.3",
    "date-fns": "^1.28.5",
    "dotenv": "^4.0.0",
    "emailjs": "^1.0.12",
    "express": "^4.15.4",
    "feed": "^1.1.0",
    "graphql-request": "^1.3.4",
    "graphql-tag": "^2.4.2",
    "handlebars": "^4.0.10",
    "html-to-text": "^3.3.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "material-ui": "v1.0.0-alpha.21",
    "material-ui-icons": "^1.0.0-beta.5",
    "mjml": "^3.3.3",
    "next": "^3.2.1",
    "node-schedule": "^1.2.4",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-apollo": "^1.4.15",
    "react-dom": "^15.6.1",
    "recompose": "^0.25.0",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "styled-components": "^2.1.2",
    "timeago-react": "^1.2.2",
    "winston": "^2.3.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/express": "^4.0.37",
    "@types/node": "^8.0.26",
    "@types/node-schedule": "^1.2.0",
    "@types/react": "^16.0.5",
    "@types/xml2js": "^0.4.0",
    "babel-eslint": "^7.2.3",
    "babel-plugin-styled-components": "^1.2.0",
    "concurrently": "^3.5.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.2.0",
    "eslint-plugin-react": "^7.3.0",
    "prettier": "^1.6.1",
    "tslint": "^5.7.0",
    "tslint-config-prettier": "^1.5.0",
    "tslint-react": "^3.2.0",
    "typescript": "^2.5.2"
  }
}
