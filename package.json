{
  "name": "octon",
  "private": true,
  "scripts": {
    "dev": "concurrently \"tsc --watch\" \"nodemon -r dotenv/config build/index.js --watch build\"",
    "build": "tsc && next build ./client",
    "start": "NODE_ENV=production node build/index.js",
    "format-client": "prettier-eslint --write \"client/**/*.js\"",
    "format-server": "prettier --single-quote --trailing-comma es5 --write \"server/**/*.ts\"",
    "format": "yarn format-client && yarn format-server",
    "lint": "eslint client && tslint server/**/*.ts client/**/*.tsx",
    "test": "npm run lint"
  },
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.17.2",
    "classnames": "^2.2.5",
    "cookie-parser": "^1.4.3",
    "date-fns": "^1.28.5",
    "dotenv": "^4.0.0",
    "emailjs": "^1.0.9",
    "express": "^4.15.3",
    "graphql-request": "^1.1.2",
    "graphql-tag": "^2.2.2",
    "handlebars": "^4.0.10",
    "html-to-text": "^3.3.0",
    "isomorphic-fetch": "^2.2.1",
    "js-cookie": "^2.1.4",
    "lodash": "^4.17.4",
    "marked": "^0.3.6",
    "material-ui": "v1.0.0-alpha.15",
    "material-ui-icons": "^1.0.0-alpha.3",
    "mjml": "^3.3.3",
    "next": "3.0.0-beta13",
    "node-schedule": "^1.2.3",
    "prop-types": "^15.5.10",
    "react": "^15.5.4",
    "react-apollo": "^1.4.2",
    "react-dom": "^15.5.4",
    "recompose": "^0.23.5",
    "request": "^2.81.0",
    "request-promise": "^4.2.1",
    "styled-components": "^2.0.1",
    "timeago-react": "^1.2.2",
    "winston": "^2.3.1",
    "xml2js": "^0.4.17"
  },
  "devDependencies": {
    "@types/express": "^4.0.35",
    "@types/node-schedule": "^1.2.0",
    "@types/react": "^15.0.27",
    "@types/request-promise": "^4.1.33",
    "@types/xml2js": "^0.0.33",
    "babel-eslint": "^7.2.3",
    "concurrently": "^3.4.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-plugin-import": "^2.3.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.0.1",
    "prettier": "^1.4.4",
    "prettier-eslint-cli": "^4.1.0",
    "tslint": "^5.4.3",
    "tslint-config-prettier": "^1.0.0",
    "tslint-react": "^3.0.0",
    "typescript": "^2.3.4"
  }
}
